  "devDependencies": {
    "coffee-script": "^1.10.0",
    "jsdoc-to-markdown": "^1.3.3",
    "lodash": "^4.6.1",
    "mocha": "^2.4.5",
    "mochainon": "^1.0.0",
    "resin-lint": "^1.5.0"
  },
  "dependencies": {
    "bluebird": "^3.3.4",
    "file-disk": "^1.0.1",
    "gpt": "^1.0.0",
    "mbr": "^0.4.4"
