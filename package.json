    "Alexis Svinartchouk <alexis@balena.io>",
    "Juan Cruz Viotti <juan@balena.io>"
  ],
  "license": "MIT",
  "devDependencies": {
    "@balena/lint": "^5.1.0",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^8.0.0",
    "chai": "^4.2.0",
    "jsdoc-to-markdown": "^6.0.1",
    "mocha": "^8.0.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6"
  },
  "dependencies": {
    "file-disk": "^8.0.0",
    "gpt": "^2.0.4",
    "mbr": "^1.1.3",
    "tslib": "^2.0.0",
    "typed-error": "^3.2.0"
  }
