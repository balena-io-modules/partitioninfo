{
  "name": "partitioninfo",
  "version": "5.3.0",
  "description": "Get information about a partition from an image file",
  "main": "build/partitioninfo.js",
  "homepage": "https://github.com/resin-io-modules/partitioninfo",
  "repository": {
    "type": "git",
    "url": "git://github.com/resin-io-modules/partitioninfo.git"
  },
  "keywords": [
    "partition",
    "image",
    "information",
    "info"
  ],
  "directories": {
    "test": "tests"
  },
  "files": [
    "build/*.js",
    "build/*.d.ts",
    "build/*.js.map",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "scripts": {
    "lint": "resin-lint --typescript lib tests typings",
    "prettier": "prettier --config ./node_modules/resin-lint/config/.prettierrc --write \"lib/**/*.ts\" \"typings/**/*.ts\" \"tests/**/*.ts\"",
    "build": "tsc",
    "test": "npm run lint && npm run build && mocha -r ts-node/register tests/**/*.spec.ts",
    "readme": "jsdoc2md --template doc/README.hbs build/partitioninfo.js > README.md",
    "prepublish": "npm run test && npm run readme"
  },
  "author": "Juan Cruz Viotti <juan@resin.io>",
  "contributors": [
    "Alexis Svinartchouk <alexis@resin.io>",
    "Juan Cruz Viotti <juan@resin.io>"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.4",
    "chai": "^4.2.0",
    "jsdoc-to-markdown": "^4.0.1",
    "mocha": "^5.2.0",
    "resin-lint": "^2.0.1",
    "ts-node": "^7.0.1",
    "typescript": "^3.2.2"
  },
  "dependencies": {
    "bluebird": "^3.5.3",
    "file-disk": "^5.1.0",
    "gpt": "^2.0.0",
    "mbr": "^1.1.3",
    "typed-error": "^3.0.2"
  }
}
